{
	"$schema": "vscode://schemas/color-theme",
	"name": "Go - Sources White Red",
	"type": "light",
	"colors": {
		"editor.background": "#efefef",
		"editor.findMatchBackground": "#ff9632",
		"editor.findMatchHighlightBackground": "#ffff00",
		"editor.findRangeHighlightBackground": "#d4d4c6",
		"editor.hoverHighlightBackground": "#efefef",
		"editor.inactiveSelectionBackground": "#d4d4c6",
		"editor.lineHighlightBackground": "#efefef",
		"editor.rangeHighlightBackground": "#efefef",
		"editor.selectionBackground": "#ff5555",
		"editorCursor.foreground": "#000000",
		"peekViewEditor.matchHighlightBackground": "#ffff00",
		"peekViewResult.matchHighlightBackground": "#ffff00",
		"editorLink.activeForeground": "#cf1616",
		"statusBar.background": "#973131",
		"tab.hoverBackground": "#cf1616",
		"dropdown.background": "#cf1616"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#006600"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#43cd31"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}